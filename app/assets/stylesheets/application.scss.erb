@import "bootstrap";
@import "bootstrap-responsive";

body {
    margin-top: 60px;
    margin-bottom: 80px;
    margin-left: 20px;
    margin-right: 20px;
    select {
        margin: 0;
    }
}

#sign-in {
    width: 30%;
    margin-top: 100px;
    margin-left: auto;
    margin-right: auto;
    text-align: center;
    .control-label {
        display: none;
    }
    .controls {
        margin-left: 0px;
    }
}

#btn_login {
    width: 30%;
    margin-left: auto;
    margin-right: auto;
}

.dataTables_wrapper {
    margin-bottom: 10px;
    padding-top: 10px;
}

#orders_wrapper tr {
    height: 30px;
}

#order_picks_wrapper tr {
    height: 30px;
}

#order_details {
    width: 100%;
}

#order_details tr, #order_details td {
    padding: 0;
    font-size: 0.99em;
    text-align: left;
}

#order_details th, #order_details td {
    padding-left: 10px;
}

#order_details tr {
    height: 15px;
}

#main-content .dataTables_processing {
    top: -30px;
    width: 250px;
    font-size: 20px;
    color: black;
    border: none;
}

#order_status_select {
    margin-right: 20px;
}

#channel_filter {
    margin-right: 20px;
}

#order_information #shipping_details a {
    color: #ff0000;
}

#maintenance_table td {
    font-size: 14px;
    vertical-align: middle;
}

td img {
    margin-left: 10px;
}

.edit-button {
    margin-left: 10px;
}

.show-button {
    margin-left: 10px;
}

#maintenance_table {
    /*margin-top: 50px;*/
    input, select, textarea {
        margin: 0;
        padding-top: 5px;
        margin-bottom: 5px;

    }
}

.dataTables_filter label {
    padding-right: 10px;
    display: inline;
}

#crumbs {
    margin-top: 15px;
    margin-bottom: 15px;
}

input[type="text"], input[type="password"], input[type="datetime"], input[type="datetime-local"], input[type="date"], input[type="month"], input[type="time"], input[type="week"], input[type="number"], input[type="email"], input[type="url"], input[type="search"], input[type="tel"], input[type="color"], .uneditable-input {
    margin-bottom: 0;
}

#copyright {
    float: left;
}

.company_logo_nav_bar {
    margin-left: 60px;
    margin-top: 20px;
}

.page_title {
    font-weight: bold;
    font-size: 16px;
}

dataTables_length {
    width: 30%;
}

.ui-tabs .ui-tabs-nav {
    height: 37px;
}

.back_button {
    margin-bottom: 15px;
}

#edit_pane .ui-tabs-panel .dataTables_wrapper {
    margin-top: 20px;
}

.new-button {
    margin-left: 5px;
}

.dataTables_length {
    width: 20% !important;
    float: left;
}

.dataTables_length select {
    width: 100px;
    height: 30px;
}

.dataTables_filter {
    width: 70% !important;
    float: right;
    text-align: right;
    padding: 3px;
}

// buttons
.activate-button, .edit-button, .show-button, .deactivate-button, .delete-button, .settings-button, .no-action-button, .unlock-button {
    width: 25px;
    height: 25px;
    display: inline-block;
    margin: -1px;
    // squidge up the buttons together a bit
    text-indent: -9000px;
}

.activate-button {
    background: url(<%=asset_path("activate.png")%>) no-repeat left top;
}

.deactivate-button {
    background: url(<%=asset_path("deactivate.png")%>) no-repeat left top;
}

.delete-button {
    background: url(<%=asset_path('icons/icon_delete.png')%>) no-repeat left top;
}

.settings-button {
    background: url(<%=asset_path("settings-button.png")%>) no-repeat left top;
}

.edit-button {
    background: url(<%=asset_path('icons/icon_edit.png')%>) no-repeat left top;
}

.show-button {
    background: url(<%=asset_path('icons/icon_show.png')%>) no-repeat left top;
}

.unlock-button {
    background: url(<%=asset_path('icons/icon_unlock.png')%>) no-repeat left top;
}

.logo {
    float: right;
}

.navbar-text {
    line-height: 25px;
}

#order_information td, #order_information th {
    text-align: left;
}

#update-container #order_picks #maintenance_table {
    position: relative;
    top: -150px;
}

.unread_message {
    background-color: orange !important;
}

#group_filter {
    margin-right: 15px;
}

#shipment_weight_summary, #available_rate_prices {
    display: inline-table;
    margin-left: 20px;
    th {
        font-weight: bold;
        padding: 20px 20px 0 0;

    }

    tr {
        td, th {
            border: 1px dotted rgb(216, 216, 216);
            padding-left: 5px;
        }
    }
}

.shipment_error {
    background: lightcoral;
    /*color: white;*/
}

.ui-autocomplete {
    height: 200px;
    overflow-y: scroll;
    overflow-x: hidden;
}

#stock_location_users_table {
    width: 100% !important;
}

#sagepay_score {
    td {
        width: 20%;
    }
}

.z-tabs.medium > ul > li > a {
    font-size: 14px !important
}

.dataTables_info {
    float: left !important;
}

#information_panel {
    height: 30px;
}

.no-close .ui-dialog-titlebar-close {
    display: none;
}

.message_error {
    color: red;
    font-size: large;
}

.message_error table {
    width: 50%;
    color: black;
    vertical-align: top;
    text-align: left;
    tbody.tr.th {
        font-weight: bold !important;
    }

    tr {
        th {
            font-weight: bold;
        }
        td {
            vertical-align: top;
        }
    }
}

.fg-toolbar .paging_full_numbers {
    width: 400px !important;
}

#shipping_address_map {
    .gm-style img {
        max-width: none;
    }
    .gm-style label {
        width: auto;
        display: inline;
    }
}

#addresses_tab #customer_addresses, #order_history_tab #order_history {
    width: 100% !important;
}

#tracking_information {
    padding: 20px;
}

#customer_tabs .dataTables_wrapper {
    margin-top: 30px;
}

#tbl_picks .line_qty {
    width: 100px;
}

#quick_filter input {
    margin-right: 30px;
}

#order_status_filter {
    display: inline;
}

.btn-file {
    position: relative;
    overflow: hidden;
}

.btn-file input[type=file] {
    position: absolute;
    top: 0;
    right: 0;
    min-width: 100%;
    min-height: 100%;
    font-size: 100px;
    text-align: right;
    filter: alpha(opacity=0);
    opacity: 0;
    background: red;
    cursor: inherit;
    display: block;
}

